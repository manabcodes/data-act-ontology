<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/dataact#"
     xml:base="http://www.semanticweb.org/dataact"
     xmlns:dpv="https://w3c.github.io/dpv/2.2/dpv/#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:odrl="https://www.w3.org/ns/odrl/2/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    
    <!-- ============================================ -->
    <!-- B2C CONTRACT: Alice's Thermostat Data       -->
    <!-- Article 4(1) - User Access Rights           -->
    <!-- ============================================ -->
    
    <!-- B2C Data Sharing Instance -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#b2cSharing_Alice">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#B2CDataSharing"/>
        <governedBy rdf:resource="http://www.semanticweb.org/dataact#b2cContract_Alice"/>
        <rdfs:label>B2C Data Sharing - Alice Thermostat</rdfs:label>
        <rdfs:comment>Article 4(1) - Consumer access to smart thermostat data</rdfs:comment>
    </owl:NamedIndividual>
    
    <!-- B2C Contract/Agreement -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#b2cContract_Alice">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#DataSharingAgreement"/>
        <dpv:hasRecipient rdf:resource="http://www.semanticweb.org/dataact#alice"/>
        <agreementDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2025-01-15T10:00:00</agreementDate>
        <compensationAmount rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.00</compensationAmount>
        <hasDescription>Free user access to thermostat data per Article 4(1) - without undue delay, continuously and in real-time</hasDescription>
        <rdfs:label>B2C Contract Alice-Thermostat</rdfs:label>
    </owl:NamedIndividual>
    
    <!-- User: Alice (Consumer) -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#alice">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#ConsumerUser"/>
        <rdf:type rdf:resource="https://w3c.github.io/dpv/2.2/dpv/#DataSubject"/>
        <ownsOrUses rdf:resource="http://www.semanticweb.org/dataact#smartThermostat1"/>
        <requestsAccessTo rdf:resource="http://www.semanticweb.org/dataact#aliceTemperatureData"/>
        <hasAccessRight rdf:resource="http://www.semanticweb.org/dataact#aliceAccessRight"/>
        <rdfs:label>Alice (Consumer)</rdfs:label>
    </owl:NamedIndividual>
    
    <!-- Data Holder: Thermostat Manufacturer -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#thermostatManufacturer">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#Manufacturer"/>
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#DataHolder"/>
        <dpv:hasData rdf:resource="http://www.semanticweb.org/dataact#aliceTemperatureData"/>
        <performsLegalAction rdf:resource="http://www.semanticweb.org/dataact#dataProvisionToAlice"/>
        <rdfs:label>Smart Home Solutions Inc (Manufacturer)</rdfs:label>
    </owl:NamedIndividual>
    
    <!-- Connected Product: Smart Thermostat -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#smartThermostat1">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#ConnectedProduct"/>
        <generatesData rdf:resource="http://www.semanticweb.org/dataact#aliceTemperatureData"/>
        <manufacturerIdentifier>THERMO-2024-001</manufacturerIdentifier>
        <productCategory>Smart Home</productCategory>
        <rdfs:label>Smart Thermostat Model X100</rdfs:label>
    </owl:NamedIndividual>
    
    <!-- Product Data: Temperature Data -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#aliceTemperatureData">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#ProductData"/>
        <rdf:type rdf:resource="https://w3c.github.io/dpv/2.2/dpv/#PersonalData"/>
        <isGeneratedBy rdf:resource="http://www.semanticweb.org/dataact#smartThermostat1"/>
        <dpv:hasDataSubject rdf:resource="http://www.semanticweb.org/dataact#alice"/>
        <isMachineReadable rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</isMachineReadable>
        <dataFormat>JSON</dataFormat>
        <rdfs:label>Alice's Temperature and Usage Data</rdfs:label>
        <rdfs:comment>Includes: temperature readings, heating patterns, energy consumption</rdfs:comment>
    </owl:NamedIndividual>
    
    <!-- Access Right -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#aliceAccessRight">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#UserAccessRight"/>
        <applicableArticle>Article 4(1)</applicableArticle>
        <hasDescription>Right to access product data without undue delay, free of charge</hasDescription>
        <rdfs:label>Alice's Data Access Right</rdfs:label>
    </owl:NamedIndividual>
    
    <!-- Data Provision Action (COMPLIANT) -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#dataProvisionToAlice">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#DataProvision"/>
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#LegalAction"/>
        <hasDescription>Manufacturer provided thermostat data to Alice via mobile app API</hasDescription>
        <actionType>API Data Access</actionType>
        <rdfs:label>Data Provision to Alice</rdfs:label>
    </owl:NamedIndividual>
    
    <!-- Article 4(1) Obligation -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#b2cObligation_Alice">
        <rdf:type rdf:resource="https://www.w3.org/ns/odrl/2/Duty"/>
        <odrl:assignee rdf:resource="http://www.semanticweb.org/dataact#thermostatManufacturer"/>
        <hasBeneficiary rdf:resource="http://www.semanticweb.org/dataact#alice"/>
        <specifiesAction rdf:resource="http://www.semanticweb.org/dataact#provideDataAction"/>
        <deonticModality>Obligation</deonticModality>
        <hasDescription>Manufacturer MUST provide thermostat data to Alice without undue delay, free of charge, continuously and in real-time</hasDescription>
        <rdfs:label>Article 4(1) Obligation - Provide Data to User</rdfs:label>
    </owl:NamedIndividual>
    
    <!-- Generic Provide Data Action -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#provideDataAction">
        <rdf:type rdf:resource="https://www.w3.org/ns/odrl/2/Action"/>
        <hasDescription>Make data available to requestor</hasDescription>
        <rdfs:label>Provide Data Action</rdfs:label>
    </owl:NamedIndividual>
    
</rdf:RDF>