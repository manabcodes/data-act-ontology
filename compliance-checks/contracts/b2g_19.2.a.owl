<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/dataact#"
     xml:base="http://www.semanticweb.org/dataact"
     xmlns:dpv="https://w3c.github.io/dpv/2.2/dpv/#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:odrl="https://www.w3.org/ns/odrl/2/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    
    <!-- ============================================ -->
    <!-- B2G CONTRACT: Public Health Emergency        -->
    <!-- Article 14-15 - Exceptional Need             -->
    <!-- Article 19(2)(a) - Prohibition on Competition -->
    <!-- ============================================ -->
    
    <!-- B2G Data Sharing Instance -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#b2gSharing_HealthEmergency">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#B2GDataSharing"/>
        <governedBy rdf:resource="http://www.semanticweb.org/dataact#b2gContract_HealthEmergency"/>
        <respondsTo rdf:resource="http://www.semanticweb.org/dataact#publicHealthEmergency"/>
        <rdfs:label>B2G Data Sharing - COVID-19 Emergency</rdfs:label>
        <rdfs:comment>Article 14-15 - Government access to medical device data for exceptional public health need</rdfs:comment>
    </owl:NamedIndividual>
    
    <!-- B2G Contract/Agreement -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#b2gContract_HealthEmergency">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#DataSharingAgreement"/>
        <dpv:hasRecipient rdf:resource="http://www.semanticweb.org/dataact#nationalHealthAuthority"/>
        <agreementDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2024-11-01T00:00:00</agreementDate>
        <compensationAmount rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">5000.00</compensationAmount>
        <hasDeadline rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-03-31</hasDeadline>
        <hasDescription>Emergency access to ventilator performance data for optimizing healthcare resource allocation during pandemic - limited compensation for data provision</hasDescription>
        <rdfs:label>B2G Contract - Health Emergency Data Access</rdfs:label>
    </owl:NamedIndividual>
    
    <!-- Exceptional Need: Public Health Emergency -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#publicHealthEmergency">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#ExceptionalNeed"/>
        <hasDescription>COVID-19 pandemic requires urgent access to medical device data for resource allocation and patient outcome optimization</hasDescription>
        <rdfs:label>Public Health Emergency (COVID-19)</rdfs:label>
        <rdfs:comment>Article 15 - Exceptional need justifying B2G data request</rdfs:comment>
    </owl:NamedIndividual>
    
    <!-- Public Sector Body: National Health Authority -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#nationalHealthAuthority">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#PublicSectorBody"/>
        <hasPartyType rdf:resource="http://www.semanticweb.org/dataact#publicBodyType"/>
        <rdfs:label>National Health Authority</rdfs:label>
        <rdfs:comment>Government body responsible for public health crisis management</rdfs:comment>
    </owl:NamedIndividual>
    
    <!-- Public Body Type -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#publicBodyType">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#PublicSectorBody"/>
        <rdfs:label>Public Sector Body Type</rdfs:label>
    </owl:NamedIndividual>
    
    <!-- Data Holder: Medical Device Manufacturer -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#medicalDeviceManufacturer">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#Manufacturer"/>
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#DataHolder"/>
        <dpv:hasData rdf:resource="http://www.semanticweb.org/dataact#ventilatorData"/>
        <performsLegalAction rdf:resource="http://www.semanticweb.org/dataact#dataProvisionToHealth"/>
        <rdfs:label>MedTech Devices Corp (Manufacturer)</rdfs:label>
    </owl:NamedIndividual>
    
    <!-- Connected Product: Smart Ventilator -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#smartVentilator">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#ConnectedProduct"/>
        <generatesData rdf:resource="http://www.semanticweb.org/dataact#ventilatorData"/>
        <productCategory>Medical Device</productCategory>
        <manufacturerIdentifier>VENT-2024-001</manufacturerIdentifier>
        <rdfs:label>SmartBreath Ventilator V500</rdfs:label>
    </owl:NamedIndividual>
    
    <!-- Product Data: Ventilator Performance Data -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#ventilatorData">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#ProductData"/>
        <rdf:type rdf:resource="https://w3c.github.io/dpv/2.2/dpv/#NonPersonalData"/>
        <isGeneratedBy rdf:resource="http://www.semanticweb.org/dataact#smartVentilator"/>
        <isMachineReadable rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</isMachineReadable>
        <dataFormat>HL7 FHIR</dataFormat>
        <rdfs:label>Ventilator Performance and Usage Data</rdfs:label>
        <rdfs:comment>Anonymized data: oxygen levels, pressure readings, ventilation cycles, equipment utilization rates - NO patient identifiers</rdfs:comment>
    </owl:NamedIndividual>
    
    <!-- Data Provision to Health Authority (COMPLIANT) -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#dataProvisionToHealth">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#DataProvision"/>
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#LegalAction"/>
        <hasDescription>Manufacturer provided ventilator data to Health Authority via secure government portal</hasDescription>
        <rdfs:label>Data Provision to Health Authority</rdfs:label>
    </owl:NamedIndividual>
    
    <!-- Article 19(2)(a) Prohibition -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#b2gProhibition_CompetitiveUse">
        <rdf:type rdf:resource="https://www.w3.org/ns/odrl/2/Prohibition"/>
        <odrl:assignee rdf:resource="http://www.semanticweb.org/dataact#nationalHealthAuthority"/>
        <specifiesAction rdf:resource="http://www.semanticweb.org/dataact#useDataToDevelopCompetingProduct"/>
        <deonticModality>Prohibition</deonticModality>
        <hasDescription>Health Authority MUST NOT use ventilator data to develop or enhance competing medical devices that compete with the manufacturer's products</hasDescription>
        <rdfs:label>Article 19(2)(a) Prohibition - No Competitive Use</rdfs:label>
    </owl:NamedIndividual>
    
    <!-- Prohibited Action Type -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#useDataToDevelopCompetingProduct">
        <rdf:type rdf:resource="https://www.w3.org/ns/odrl/2/Action"/>
        <hasDescription>Use received data or insights to develop or enhance connected products/services that compete with data holder's offerings</hasDescription>
        <rdfs:label>Develop Competing Product Using B2G Data</rdfs:label>
    </owl:NamedIndividual>
    
    <!-- ============================================ -->
    <!-- VIOLATION SCENARIO (for testing)             -->
    <!-- Uncomment to test Article 19(2)(a) violation -->
    <!-- ============================================ -->
    
    <!-- SCENARIO: Health Authority uses data to develop public ventilator -->
    <!--
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#prohibitedAction_PublicVentilator">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#LegalAction"/>
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#UseDataToDevelopCompetingProduct"/>
        <hasDescription>Health Authority used ventilator performance insights to develop specifications for state-funded public ventilator model that competes with manufacturer's device</hasDescription>
        <actionType>Product Development</actionType>
        <rdfs:label>Prohibited Action - Public Ventilator Development</rdfs:label>
        <rdfs:comment>VIOLATION: Article 19(2)(a) - Public body cannot use B2G data to compete</rdfs:comment>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#nationalHealthAuthority">
        <performsLegalAction rdf:resource="http://www.semanticweb.org/dataact#prohibitedAction_PublicVentilator"/>
    </owl:NamedIndividual>
    -->
    
    <!-- ============================================ -->
    <!-- Alternative Class Definition for Actions     -->
    <!-- ============================================ -->
    
    <owl:Class rdf:about="http://www.semanticweb.org/dataact#UseDataToDevelopCompetingProduct">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/dataact#LegalAction"/>
        <rdfs:comment>Action prohibited by Article 19(2)(a)</rdfs:comment>
        <rdfs:label>Use Data To Develop Competing Product</rdfs:label>
    </owl:Class>
    
</rdf:RDF>