<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/dataact#"
     xml:base="http://www.semanticweb.org/dataact"
     xmlns:dpv="https://w3c.github.io/dpv/2.2/dpv/#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:odrl="https://www.w3.org/ns/odrl/2/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    
    <!-- ============================================ -->
    <!-- B2B CONTRACT: Industrial Machine Maintenance -->
    <!-- Article 8 - B2B Data Sharing with FRAND      -->
    <!-- Article 8(6) - Trade Secret Exception        -->
    <!-- ============================================ -->
    
    <!-- B2B Data Sharing Instance -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#b2bSharing_Maintenance">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#B2BDataSharing"/>
        <authorizedBy rdf:resource="http://www.semanticweb.org/dataact#factoryOwnerAcme"/>
        <governedBy rdf:resource="http://www.semanticweb.org/dataact#b2bContract_Maintenance"/>
        <rdfs:label>B2B Data Sharing - Industrial Maintenance</rdfs:label>
        <rdfs:comment>Article 8 - B2B data sharing for aftermarket maintenance services</rdfs:comment>
    </owl:NamedIndividual>
    
    <!-- B2B Contract/Agreement -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#b2bContract_Maintenance">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#DataSharingAgreement"/>
        <dpv:hasRecipient rdf:resource="http://www.semanticweb.org/dataact#mantenimientoTech"/>
        <subjectToFRAND rdf:resource="http://www.semanticweb.org/dataact#frandCondition_Maintenance"/>
        <agreementDate rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2025-02-01T14:00:00</agreementDate>
        <compensationAmount rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">1200.00</compensationAmount>
        <hasDescription>B2B data access for predictive maintenance services - 12 month subscription</hasDescription>
        <rdfs:label>B2B Contract - Maintenance Data Access</rdfs:label>
    </owl:NamedIndividual>
    
    <!-- FRAND Condition (COMPLIANT) -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#frandCondition_Maintenance">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#FRANDCondition"/>
        <isFair rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</isFair>
        <isReasonable rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</isReasonable>
        <isNonDiscriminatory rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</isNonDiscriminatory>
        <hasDescription>Fair pricing based on data preparation costs, reasonable technical requirements, non-discriminatory terms for all SME service providers</hasDescription>
        <rdfs:label>FRAND Condition - Maintenance Contract</rdfs:label>
    </owl:NamedIndividual>
    
    <!-- User: Factory Owner (authorizes B2B sharing) -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#factoryOwnerAcme">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#EnterpriseUser"/>
        <ownsOrUses rdf:resource="http://www.semanticweb.org/dataact#industrialRobot1"/>
        <rdfs:label>ACME Manufacturing Ltd (Factory Owner)</rdfs:label>
        <rdfs:comment>User who owns the industrial equipment and authorizes B2B data sharing per Article 8(4)</rdfs:comment>
    </owl:NamedIndividual>
    
    <!-- Data Holder: Sensor/Machine Manufacturer -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#sensorManufacturer">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#Manufacturer"/>
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#DataHolder"/>
        <dpv:hasData rdf:resource="http://www.semanticweb.org/dataact#machineData1"/>
        <rdfs:label>Industrial Robotics Corp (Manufacturer)</rdfs:label>
    </owl:NamedIndividual>
    
    <!-- Data Recipient: Maintenance Service Provider (SME) -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#mantenimientoTech">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#AftermarketServiceProvider"/>
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#DataRecipient"/>
        <hasPartyType rdf:resource="http://www.semanticweb.org/dataact#smeType"/>
        <rdfs:label>MantenimientoTech SL (SME Service Provider)</rdfs:label>
    </owl:NamedIndividual>
    
    <!-- SME Type -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#smeType">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#SME"/>
        <rdfs:label>Small and Medium-sized Enterprise</rdfs:label>
    </owl:NamedIndividual>
    
    <!-- Connected Product: Industrial Robot -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#industrialRobot1">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#ConnectedProduct"/>
        <generatesData rdf:resource="http://www.semanticweb.org/dataact#machineData1"/>
        <productCategory>Industrial IoT</productCategory>
        <manufacturerIdentifier>ROBOT-IND-2024-001</manufacturerIdentifier>
        <rdfs:label>Industrial Robot ARM-500</rdfs:label>
    </owl:NamedIndividual>
    
    <!-- Product Data: Machine Telemetry (NON-TRADE-SECRET VERSION) -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#machineData1">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#ProductData"/>
        <rdf:type rdf:resource="https://w3c.github.io/dpv/2.2/dpv/#NonPersonalData"/>
        <isGeneratedBy rdf:resource="http://www.semanticweb.org/dataact#industrialRobot1"/>
        <isMachineReadable rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</isMachineReadable>
        <dataFormat>CSV</dataFormat>
        <rdfs:label>Industrial Robot Telemetry Data</rdfs:label>
        <rdfs:comment>Includes: operational hours, error codes, temperature, vibration - does NOT include proprietary control algorithms</rdfs:comment>
    </owl:NamedIndividual>
    
    <!-- SCENARIO A: Data DOES NOT contain trade secrets (should share) -->
    <!-- No containsTradeSecret property = manufacturer must share -->
    
    <!-- SCENARIO B (OPTIONAL): Trade Secret Data - for testing Article 8(6) -->
    <!-- Uncomment to test trade secret exception -->
    <!--
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#machineData1_WithSecret">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#ProductData"/>
        <isGeneratedBy rdf:resource="http://www.semanticweb.org/dataact#industrialRobot1"/>
        <containsTradeSecret rdf:resource="http://www.semanticweb.org/dataact#proprietaryAlgorithm"/>
        <rdfs:label>Machine Data (including trade secrets)</rdfs:label>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#proprietaryAlgorithm">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#TradeSecretData"/>
        <hasDescription>Proprietary predictive maintenance algorithm</hasDescription>
        <rdfs:label>Trade Secret: Maintenance Algorithm</rdfs:label>
    </owl:NamedIndividual>
    -->
    
    <!-- Article 8(6) Permission (to refuse if trade secret) -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#b2bPermission_TradeSecret">
        <rdf:type rdf:resource="https://www.w3.org/ns/odrl/2/Permission"/>
        <odrl:assignee rdf:resource="http://www.semanticweb.org/dataact#sensorManufacturer"/>
        <specifiesAction rdf:resource="http://www.semanticweb.org/dataact#refuseDataAccessAction"/>
        <deonticModality>Permission</deonticModality>
        <hasDescription>Manufacturer MAY refuse to share data if it constitutes trade secrets (Article 8.6)</hasDescription>
        <rdfs:label>Article 8(6) Permission - Trade Secret Refusal</rdfs:label>
    </owl:NamedIndividual>
    
    <!-- Refuse Data Access Action -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#refuseDataAccessAction">
        <rdf:type rdf:resource="https://www.w3.org/ns/odrl/2/Action"/>
        <hasDescription>Refuse to provide requested data</hasDescription>
        <rdfs:label>Refuse Data Access</rdfs:label>
    </owl:NamedIndividual>
    
    <!-- Data Provision Action - COMMENT OUT TO TEST VIOLATION -->
    <!-- If commented, query will detect violation (refusal without trade secret) -->
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#dataProvisionToMaintenance">
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#DataProvision"/>
        <rdf:type rdf:resource="http://www.semanticweb.org/dataact#LegalAction"/>
        <hasDescription>Manufacturer provided machine telemetry data to MantenimientoTech via secure API</hasDescription>
        <rdfs:label>Data Provision to Maintenance Service</rdfs:label>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dataact#sensorManufacturer">
        <performsLegalAction rdf:resource="http://www.semanticweb.org/dataact#dataProvisionToMaintenance"/>
    </owl:NamedIndividual>
    
</rdf:RDF>