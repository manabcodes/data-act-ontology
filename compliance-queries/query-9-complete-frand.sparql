# CHECK 9: Complete FRAND Compliance
# Verify all three FRAND conditions together
# Expected: Shows which agreements pass/fail complete FRAND test

PREFIX dataact: <http://www.semanticweb.org/dataact#>

SELECT ?agreement ?contract ?price
       ?isFair ?isReasonable ?isNonDiscrim
       ((?isFair && ?isReasonable && ?isNonDiscrim) AS ?compliant)
WHERE {
  ?agreement a dataact:B2BDataSharing ;
             dataact:governedBy ?contract .
  
  ?contract dataact:compensationAmount ?price ;
            dataact:subjectToFRAND ?frand .
  
  ?frand dataact:isFair ?isFair ;
         dataact:isReasonable ?isReasonable ;
         dataact:isNonDiscriminatory ?isNonDiscrim .
}
ORDER BY ?compliant