# CHECK 3: FRAND Fairness (Article 8.1)
# Find agreements with unfair FRAND terms
# Expected: agreement247 with isFair = false

PREFIX dataact: <http://www.semanticweb.org/dataact#>

SELECT ?agreement ?contract ?isFair ?isReasonable ?isNonDiscrim
WHERE {
  ?agreement a dataact:B2BDataSharing ;
             dataact:governedBy ?contract .
  
  ?contract dataact:subjectToFRAND ?frand .
  
  ?frand dataact:isFair ?isFair ;
         dataact:isReasonable ?isReasonable ;
         dataact:isNonDiscriminatory ?isNonDiscrim .
  
  # At least one FRAND requirement fails
  FILTER(?isFair = false)
}